@article{Blau2001,
abstract = {Circadian rhythms are regulated by endogenous body clocks, which are formed by rhythmic cycles of clock gene expression. Almost all reviews of the Drosophila circadian clock state that the intracellular oscillator is based on a simple negative feedback loop. However, not many 'simple' feedback loops in biology last for 24 h. Instead, the Drosophila clock is a series of precisely timed steps that are deliberately slow. In this paper, I will discuss the current model for how the Drosophila clock is regulated, and ask what questions remain to be answered.},
author = {Blau, J},
doi = {10.1006/scdb.2001.0256},
issn = {1084-9521},
