@article{Allada2010,
abstract = {Circadian clocks organize behavior and physiology to adapt to daily environmental cycles. Genetic approaches in the fruit fly, Drosophila melanogaster, have revealed widely conserved molecular gears of these 24-h timers. Yet much less is known about how these cell-autonomous clocks confer temporal information to modulate cellular functions. Here we discuss our current knowledge of circadian clock function in Drosophila, providing an overview of the molecular underpinnings of circadian clocks. We then describe the neural network important for circadian rhythms of locomotor activity, including how these molecular clocks might influence neuronal function. Finally, we address a range of behaviors and physiological systems regulated by circadian clocks, including discussion of specific peripheral oscillators and key molecular effectors where they have been described. These studies reveal a remarkable complexity to circadian pathways in this "simple" model organism.},
author = {Allada, Ravi and Chung, Brian Y},
doi = {10.1146/annurev-physiol-021909-135815},
isbn = {0219091358},
issn = {1545-1585},
journal = {Annual review of physiology},
keywords = {Animals,Behavior, Animal,Behavior, Animal: physiology,Biological Clocks,Biological Clocks: genetics,Biological Clocks: physiology,Circadian Rhythm,Circadian Rhythm: genetics,Circadian Rhythm: immunology,Circadian Rhythm: physiology,Drosophila,Drosophila: immunology,Drosophila: physiology,Humans,Locomotion,Locomotion: physiology,Nerve Net,Nerve Net: physiology},
month = mar,
pages = {605--24},
pmid = {20148690},
title = {{Circadian organization of behavior and physiology in Drosophila.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2887282\&tool=pmcentrez\&rendertype=abstract},
volume = {72},
year = {2010}
}
